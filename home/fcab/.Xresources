!!!!!!! Xft /fonts !!!!!!!!
Xft.dpi:	96
Xft.antialias:	true
Xft.rgba:	rgb
Xft.hinting:	true
Xft.hintstyle:	hintslight





!!!!!!!!!!!!!! URxvt !!!!!!!!!!!

URxvt.depth:	32
URxvt.loginShell:	true




!!font
URxvt*font:	xft:Monaco:pixelsize=15
!URxvt.font:	xft:monospace:size=16
URxvt*boldFont:	xft:Monaco:bold:pixelsize=15
URxvt*letterSpace:	-1

!!scrollbar
URxvt*scrollBar:	true

!styles: rxvt(default), plain, next, xterm
URxvt*scrollstyle:	plain

!! Bindings !!
!URxvt.keysym.Control-Up:     \033[1;5A
!URxvt.keysym.Control-Down:   \033[1;5B
!URxvt.keysym.Control-Left:   \e[1;5D
!URxvt.keysym.Control-Right:	\e[1;5C



!!------------------ Extensions----------

URxvt.perl-ext-common: resize-font

!!---- Extensions Config -----
!! resize-font
URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k


!! Theme !!


! Cursor
URxvt.cursorBlink:          false
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true


